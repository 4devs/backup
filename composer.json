{
    "name": "fdevs/backup",
    "type": "library",
    "description": "FDevs Backup Component",
    "keywords": [
        "backup",
        "dump",
        "reestore",
        "mysqldump",
        "mysqlrestore",
        "mongodump",
        "mongorestore"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Samusev",
            "email": "andrey_simfi@list.ru",
            "homepage": "http://andrey.4devs.io"
        },
        {
            "name": "4devs Community",
            "homepage": "https://github.com/4devs/backup/contributors"
        }
    ],
    "require": {
        "symfony/process": "~2.8|~3.0|~4.0",
        "symfony/filesystem": "~2.8|~3.0|~4.0",
        "symfony/options-resolver": "~2.8|~3.0|~4.0",
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "knplabs/gaufrette": "^0.2.1"
    },
    "suggest": {
        "knplabs/gaufrette": "use with different filesystem"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "FDevs\\Backup\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
